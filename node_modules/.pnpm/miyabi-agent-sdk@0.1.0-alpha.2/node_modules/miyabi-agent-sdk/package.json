  "type": "module",
  "bin": {
    "miyabi": "./dist/cli/miyabi.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:e2e": "node dist/e2e/run-e2e-tests.js",
    "test:e2e:real": "node dist/e2e/run-e2e-real.js",
    "demo:record": "playwright test demo/miyabi-workflow-demo.spec.ts",
    "demo:show": "playwright show-report demo/playwright-report",
