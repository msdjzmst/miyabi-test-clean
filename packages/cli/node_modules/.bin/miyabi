#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/dist/cli/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/dist/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/dist/cli/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/dist/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules/miyabi-agent-sdk/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/miyabi-agent-sdk@0.1.0-alpha.2/node_modules:/home/runner/work/miyabi-test-clean/miyabi-test-clean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../miyabi-agent-sdk/dist/cli/miyabi.js" "$@"
else
  exec node  "$basedir/../miyabi-agent-sdk/dist/cli/miyabi.js" "$@"
fi
